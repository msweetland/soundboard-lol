{
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "extends": "airbnb",
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": 0,
    "comma-spacing": 0,
    "indent": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/no-autofocus": 1,
    "jsx-a11y/no-noninteractive-element-interactions": 1,
    "jsx-a11y/no-static-element-interactions": 0,
    "max-len": 1,
    "no-bitwise": 0,
    "no-class-assign": 0,
    "no-console": 0,
    "no-else-return": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 1,
    "no-plusplus": 0,
    "no-template-curly-in-string": 1,
    "no-unused-expressions": 1,
    "no-unused-vars": 1,
    "no-useless-escape": 0,
    "object-curly-spacing": 0,
    "prefer-const": 0,
    "quote-props": 1,
    "react/no-did-mount-set-state": 1,
    "react/prop-types": 1,
    "space-in-parens": 0
  },
  "globals": {
    "document": {},
    "fetch": {},
    "window": {}
  }
}